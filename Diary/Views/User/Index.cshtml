@using System.Threading
@{
    ViewData["Title"] = "Index";
}
<head>
    <link rel="stylesheet" href="~/css/index.css" />
</head>
<body>
<a id="btn-h" asp-controller="User" asp-action="Account"><button class="btn btn-warning btn-block">Account</button></a>
<svg class="intro" viewbox="0 0 200 86">
    <text text-anchor="start" x="10" y="30" class="text text-stroke" clip-path="url(#text1)">WELCOME</text>
    <text text-anchor="start" x="10" y="50" class="text text-stroke" clip-path="url(#text2)">@ViewBag.User.Fname @ViewBag.User.Lname</text>
    <text text-anchor="start" x="10" y="30" class="text text-stroke text-stroke-2" clip-path="url(#text1)">WELCOME</text>
    <text text-anchor="start" x="10" y="50" class="text text-stroke text-stroke-2" clip-path="url(#text2)">@ViewBag.User.Fname @ViewBag.User.Lname</text>
    <defs>
        <clipPath id="text1">
            <text text-anchor="start" x="10" y="30" class="text">WELCOME</text>
        </clipPath>
        <clipPath id="text2">
            <text text-anchor="start" x="10" y="50" class="text">@ViewBag.User.Fname @ViewBag.User.Lname</text>
        </clipPath>

    </defs>
        
</svg>

</body>
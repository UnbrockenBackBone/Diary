@model IEnumerable<Diary.Models.Event>

@{
    ViewData["Title"] = "Events";
}

<h1>Events</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name_Event)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Start_Date_Event)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.End_Date_Event)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name_Event)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Start_Date_Event)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.End_Date_Event)
            </td>
        </tr>
}
    </tbody>
</table>
